#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/max/Documents/GitHub/operations-toolkit/node_modules/.pnpm/prisma@7.4.0_@types+react@19.2.14_magicast@0.5.2_react-dom@19.2.3_react@19.2.3__react@19.2.3_typescript@5.9.3/node_modules/prisma/build/node_modules:/Users/max/Documents/GitHub/operations-toolkit/node_modules/.pnpm/prisma@7.4.0_@types+react@19.2.14_magicast@0.5.2_react-dom@19.2.3_react@19.2.3__react@19.2.3_typescript@5.9.3/node_modules/prisma/node_modules:/Users/max/Documents/GitHub/operations-toolkit/node_modules/.pnpm/prisma@7.4.0_@types+react@19.2.14_magicast@0.5.2_react-dom@19.2.3_react@19.2.3__react@19.2.3_typescript@5.9.3/node_modules:/Users/max/Documents/GitHub/operations-toolkit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/max/Documents/GitHub/operations-toolkit/node_modules/.pnpm/prisma@7.4.0_@types+react@19.2.14_magicast@0.5.2_react-dom@19.2.3_react@19.2.3__react@19.2.3_typescript@5.9.3/node_modules/prisma/build/node_modules:/Users/max/Documents/GitHub/operations-toolkit/node_modules/.pnpm/prisma@7.4.0_@types+react@19.2.14_magicast@0.5.2_react-dom@19.2.3_react@19.2.3__react@19.2.3_typescript@5.9.3/node_modules/prisma/node_modules:/Users/max/Documents/GitHub/operations-toolkit/node_modules/.pnpm/prisma@7.4.0_@types+react@19.2.14_magicast@0.5.2_react-dom@19.2.3_react@19.2.3__react@19.2.3_typescript@5.9.3/node_modules:/Users/max/Documents/GitHub/operations-toolkit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../prisma/build/index.js" "$@"
else
  exec node  "$basedir/../prisma/build/index.js" "$@"
fi
