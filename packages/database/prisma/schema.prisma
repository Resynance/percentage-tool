  timeReportId      String   @map("time_report_id")
  workType          String   @map("work_type") // TASK or QA
  workDescription   String   @map("work_description") @db.Text
  estimatedMinutes  Int      @map("estimated_minutes")
  confidenceScore   Decimal? @map("confidence_score") @db.Decimal(3, 2)
  reasoning         String?  @db.Text
  llmModel          String?  @map("llm_model")
  llmProvider       String?  @map("llm_provider")
  llmCost           Decimal? @map("llm_cost") @db.Decimal(10, 6)
  createdAt         DateTime @default(now()) @map("created_at") @db.Timestamptz(6)

  // Relations
  timeReport TimeReportRecord @relation(fields: [timeReportId], references: [id], onDelete: Cascade)

  @@index([timeReportId])
  @@index([workType])
  @@map("time_estimates")
  @@schema("public")
}

/**
 * MEETING CLAIM: LLM-extracted meeting claims from worker notes.
 * Includes verification against the billable meetings database.
 */
model MeetingClaim {
  id                     String    @id @default(dbgenerated("gen_random_uuid()::text"))
  timeReportId           String    @map("time_report_id")
  claimedMeetingName     String    @map("claimed_meeting_name")
  claimedDurationMinutes Int?      @map("claimed_duration_minutes")
  extractionConfidence   Decimal?  @map("extraction_confidence") @db.Decimal(3, 2)
  verified               Boolean   @default(false)
  matchedMeetingId       String?   @map("matched_meeting_id")
  verificationNotes      String?   @map("verification_notes") @db.Text
  llmModel               String?   @map("llm_model")
  llmProvider            String?   @map("llm_provider")
  llmCost                Decimal?  @map("llm_cost") @db.Decimal(10, 6)
  createdAt              DateTime  @default(now()) @map("created_at") @db.Timestamptz(6)
  verifiedAt             DateTime? @map("verified_at") @db.Timestamptz(6)

  // Relations
  timeReport     TimeReportRecord @relation(fields: [timeReportId], references: [id], onDelete: Cascade)
  matchedMeeting BillableMeeting? @relation(fields: [matchedMeetingId], references: [id])

  @@index([timeReportId])
  @@index([verified])
  @@index([matchedMeetingId])
  @@map("meeting_claims")
  @@schema("public")
}

/**
 * QUALITY SCORE: LLM-generated quality assessments for work.
 * Used in combination with time analysis for better flagging decisions.
 */
model QualityScore {
  id                 String   @id @default(dbgenerated("gen_random_uuid()::text"))
  timeReportId       String   @map("time_report_id")
  workType           String   @map("work_type") // TASK or QA
  workDescription    String   @map("work_description") @db.Text
  qualityScore       Decimal  @map("quality_score") @db.Decimal(3, 1)
  qualityReasoning   String?  @map("quality_reasoning") @db.Text
  completenessScore  Decimal? @map("completeness_score") @db.Decimal(3, 1)
  accuracyScore      Decimal? @map("accuracy_score") @db.Decimal(3, 1)
  clarityScore       Decimal? @map("clarity_score") @db.Decimal(3, 1)
  llmModel           String?  @map("llm_model")
  llmProvider        String?  @map("llm_provider")
  llmCost            Decimal? @map("llm_cost") @db.Decimal(10, 6)
  createdAt          DateTime @default(now()) @map("created_at") @db.Timestamptz(6)

  // Relations
  timeReport TimeReportRecord @relation(fields: [timeReportId], references: [id], onDelete: Cascade)

  @@index([timeReportId])
  @@index([workType])
  @@index([qualityScore])
  @@map("quality_scores")
  @@schema("public")
}

/**
 * TIME ANALYSIS FLAG: Workers flagged for time discrepancies or quality issues.
 * Requires manual review by Fleet/Admin.
 */
model TimeAnalysisFlag {
  id                     String    @id @default(dbgenerated("gen_random_uuid()::text"))
  timeReportId           String    @map("time_report_id")
  workerName             String    @map("worker_name")
  workerEmail            String    @map("worker_email")
  workDate               DateTime  @map("work_date") @db.Date
  flagType               String    @map("flag_type") // TIME_DISCREPANCY, LOW_QUALITY, etc.
  severity               FlagSeverity
  status                 FlagStatus   @default(PENDING)
  expectedHours          Decimal?  @map("expected_hours") @db.Decimal(10, 5)
  actualHours            Decimal?  @map("actual_hours") @db.Decimal(10, 5)
  discrepancyPercentage  Decimal?  @map("discrepancy_percentage") @db.Decimal(5, 2)
  meetingHoursClaimed    Decimal?  @map("meeting_hours_claimed") @db.Decimal(10, 5)
  meetingHoursVerified   Decimal?  @map("meeting_hours_verified") @db.Decimal(10, 5)
  averageQualityScore    Decimal?  @map("average_quality_score") @db.Decimal(3, 1)
  flagReason             String    @map("flag_reason") @db.Text
  analysisThreshold      Decimal?  @map("analysis_threshold") @db.Decimal(5, 2)
  reviewedById           String?   @map("reviewed_by_id") @db.Uuid
  reviewedAt             DateTime? @map("reviewed_at") @db.Timestamptz(6)
  resolutionNotes        String?   @map("resolution_notes") @db.Text
  createdAt              DateTime  @default(now()) @map("created_at") @db.Timestamptz(6)
  updatedAt              DateTime  @default(now()) @updatedAt @map("updated_at") @db.Timestamptz(6)

  // Relations
  timeReport TimeReportRecord @relation(fields: [timeReportId], references: [id], onDelete: Cascade)
  reviewedBy Profile?         @relation(fields: [reviewedById], references: [id])

  @@index([workerEmail])
  @@index([status])
  @@index([severity])
  @@index([workDate])
  @@index([flagType])
  @@map("time_analysis_flags")
  @@schema("public")
}

/**
 * TIME ANALYSIS CONFIG: Configurable thresholds and settings for analysis.
 * Only one configuration can be active at a time.
 */
model TimeAnalysisConfig {
  id                        String   @id @default(dbgenerated("gen_random_uuid()::text"))
  configName                String   @unique @map("config_name")
  timeDiscrepancyThreshold  Decimal  @default(15.00) @map("time_discrepancy_threshold") @db.Decimal(5, 2)
  taskTimeMin               Int      @default(45) @map("task_time_min")
  taskTimeMax               Int      @default(60) @map("task_time_max")
  qaTimeMin                 Int      @default(5) @map("qa_time_min")
  qaTimeMax                 Int      @default(20) @map("qa_time_max")
  minAcceptableQuality      Decimal? @default(6.0) @map("min_acceptable_quality") @db.Decimal(3, 1)
  isActive                  Boolean  @default(false) @map("is_active")
  createdAt                 DateTime @default(now()) @map("created_at") @db.Timestamptz(6)
  updatedAt                 DateTime @default(now()) @updatedAt @map("updated_at") @db.Timestamptz(6)
  createdById               String?  @map("created_by_id") @db.Uuid

  // Relations
  createdBy Profile? @relation(fields: [createdById], references: [id])

  @@map("time_analysis_configs")
  @@schema("public")
}

// Enums for time analysis
enum FlagSeverity {
  LOW
  MEDIUM
  HIGH
  CRITICAL

  @@map("flag_severity")
  @@schema("public")
}

enum FlagStatus {
  PENDING
  UNDER_REVIEW
  RESOLVED
  DISMISSED

  @@map("flag_status")
  @@schema("public")
}

/**
 * PROMPT AUTHENTICITY RECORDS: Stores prompts and their analysis results.
 * Used for detecting non-native speaker patterns and AI-generated content.
 */
model PromptAuthenticityRecord {
  id                     String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  versionId              String    @unique @map("version_id")
  taskKey                String    @map("task_key")
  prompt                 String    @db.Text
  versionNo              Int?      @map("version_no")
  isActive               Boolean?  @map("is_active")
  createdByName          String?   @map("created_by_name")
  createdByEmail         String?   @map("created_by_email")
  createdAt              DateTime? @map("created_at") @db.Timestamptz(6)
  envKey                 String?   @map("env_key")
  taskLifecycleStatus    String?   @map("task_lifecycle_status")
  taskModality           String?   @map("task_modality")
  scenarioTitle          String?   @map("scenario_title")
  taskComplexityTier     String?   @map("task_complexity_tier")

  // Analysis results
  analysisStatus         String    @default("PENDING") @map("analysis_status")
  isLikelyNonNative      Boolean?  @map("is_likely_non_native")
  nonNativeConfidence    Decimal?  @map("non_native_confidence") @db.Decimal(5, 2)
  nonNativeIndicators    Json?     @map("non_native_indicators")
  isLikelyAIGenerated    Boolean?  @map("is_likely_ai_generated")
  aiGeneratedConfidence  Decimal?  @map("ai_generated_confidence") @db.Decimal(5, 2)
  aiGeneratedIndicators  Json?     @map("ai_generated_indicators")
  overallAssessment      String?   @map("overall_assessment") @db.Text
  recommendations        Json?

  // Metadata
  llmModel               String?   @map("llm_model")
  llmProvider            String?   @map("llm_provider")
  llmCost                Decimal?  @map("llm_cost") @db.Decimal(10, 6)
  analyzedAt             DateTime? @map("analyzed_at") @db.Timestamptz(6)
  errorMessage           String?   @map("error_message") @db.Text

  // Timestamps
  importedAt             DateTime  @default(now()) @map("imported_at") @db.Timestamptz(6)
  updatedAt              DateTime  @default(now()) @updatedAt @map("updated_at") @db.Timestamptz(6)

  @@index([analysisStatus])
  @@index([versionId])
  @@index([taskKey])
  @@index([createdByEmail])
  @@index([isLikelyNonNative])
  @@index([isLikelyAIGenerated])
  @@map("prompt_authenticity_records")
  @@schema("public")
}

/**
 * PROMPT AUTHENTICITY JOBS: Background job tracking for batch analysis.
 */
model PromptAuthenticityJob {
  id                   String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  status               String    @default("PENDING")
  totalPrompts         Int       @default(0) @map("total_prompts")
  analyzedPrompts      Int       @default(0) @map("analyzed_prompts")
  failedPrompts        Int       @default(0) @map("failed_prompts")
  flaggedNonNative     Int       @default(0) @map("flagged_non_native")
  flaggedAIGenerated   Int       @default(0) @map("flagged_ai_generated")
  totalCost            Decimal   @default(0) @map("total_cost") @db.Decimal(10, 6)
  batchSize            Int       @default(200) @map("batch_size")

  // Date range filters (optional)
  filterStartDate      DateTime? @map("filter_start_date") @db.Timestamptz(6)
  filterEndDate        DateTime? @map("filter_end_date") @db.Timestamptz(6)

  // Job metadata
  startedAt            DateTime? @map("started_at") @db.Timestamptz(6)
  completedAt          DateTime? @map("completed_at") @db.Timestamptz(6)
  pausedAt             DateTime? @map("paused_at") @db.Timestamptz(6)
  cancelledAt          DateTime? @map("cancelled_at") @db.Timestamptz(6)
  lastHeartbeat        DateTime? @map("last_heartbeat") @db.Timestamptz(6)
  errorMessage         String?   @map("error_message") @db.Text

  // Timestamps
  createdAt            DateTime  @default(now()) @map("created_at") @db.Timestamptz(6)
  updatedAt            DateTime  @default(now()) @updatedAt @map("updated_at") @db.Timestamptz(6)

  @@index([status])
  @@map("prompt_authenticity_jobs")
  @@schema("public")
}

/**
 * GUIDELINE: PDF-based guidelines for alignment analysis.
 * Can be associated with specific environments or left as default/global (environment = null).
 * Used by alignment scoring system to evaluate task/feedback quality.
 */
model Guideline {
  id           String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  name         String   // User-friendly name: "Production Quality Guidelines v2.1"
  content      String   @db.Text // Base64-encoded PDF content
  environments String[] @default([]) // Array of environment associations (empty = global/all)
  uploadedBy   String   @map("uploaded_by") @db.Uuid
  createdAt    DateTime @default(now()) @map("created_at") @db.Timestamptz(6)
  updatedAt    DateTime @default(now()) @updatedAt @map("updated_at") @db.Timestamptz(6)

  users        users    @relation(fields: [uploadedBy], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@index([uploadedBy])
  @@index([createdAt(sort: Desc)])
  @@map("guidelines")
  @@schema("public")
}
