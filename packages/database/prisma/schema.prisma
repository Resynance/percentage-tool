  raterGroupId String @map("rater_group_id")
  userId       String @map("user_id") @db.Uuid

  joinedAt DateTime @default(now()) @map("joined_at")

  // Relations
  raterGroup RaterGroup @relation(fields: [raterGroupId], references: [id], onDelete: Cascade)
