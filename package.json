{
  "name": "operations-toolkit-monorepo",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --concurrency=15",
    "dev:user": "turbo run dev --filter=@repo/user-app",
    "dev:qa": "turbo run dev --filter=@repo/qa-app",
    "dev:core": "turbo run dev --filter=@repo/core-app",
    "dev:fleet": "turbo run dev --filter=@repo/fleet-app",
    "dev:admin": "turbo run dev --filter=@repo/admin-app",
    "dev:supabase": "supabase start",
    "dev:stop": "supabase stop",
    "dev:reset": "supabase db reset",
    "dev:studio": "supabase studio",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e",
    "test:ci": "turbo run test test:e2e",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean && rm -rf node_modules",
    "db:generate": "turbo run db:generate",
    "db:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "guides:pdf": "node scripts/generate-guide-pdfs.mjs"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^3.3.0",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.93.2",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "pdf-parse": "^1.1.1",
    "pg": "^8.17.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@next/env": "^16.1.6",
    "@playwright/test": "^1.58.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/pdf-parse": "^1.1.4",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.18",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "jsdom": "^27.4.0",
    "md-to-pdf": "^5.2.5",
    "prisma": "^7.3.0",
    "tailwindcss": "^4",
    "turbo": "^2.8.3",
    "typescript": "^5",
    "vitest": "^4.0.18"
  }
}
